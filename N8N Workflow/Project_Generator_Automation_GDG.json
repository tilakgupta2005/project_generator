    {
      "parameters": {
        "jsCode": "// --- Combine your JSON data ---\nconst project = $input.first().json.output  || {}; // Second JSON: project info\n\n// --- Extract data safely ---\nconst name = $('Split Out').first().json['Full Name'];\nconst email = $('Wait').first().json['Email Address'];\nconst linkedin = $('Wait').first().json.Linkedin || \"#\";\nconst github = $('Wait').first().json.Github  || \"#\";\n\nconst role = project.role || \"Developer\";\nconst projectTitle = project.project_title || \"Untitled Project\";\nconst problem = project.problem_it_solves || \"No problem statement provided.\";\nconst techStack = project.tech_stack || [];\nconst shortGuide = project.short_guide || [];\n\n// --- Build the HTML based on your original code ---\nconst htmlContent = `\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>${projectTitle}</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <script src=\"https://unpkg.com/lucide@latest\"></script>\n    <link href=\"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap\" rel=\"stylesheet\">\n    <style>body { font-family: 'Inter', sans-serif; }</style>\n</head>\n<body class=\"bg-gray-100 text-gray-800\">\n\n    <div class=\"container mx-auto max-w-6xl p-4 sm:p-6 lg:p-8\">\n        <div class=\"flex flex-col lg:flex-row lg:gap-8\">\n\n            <!-- === USER PROFILE BOX === -->\n            <aside class=\"w-full lg:w-1/3 lg:sticky lg:top-8 mb-8 lg:mb-0\">\n                <div class=\"bg-white rounded-xl shadow-lg p-6\">\n                    <div class=\"flex flex-col items-center\">\n                        <img src=\"https://placehold.co/128x128/6366f1/white?text=${name.charAt(0).toUpperCase()}\" \n                             alt=\"User Profile Picture\"\n                             class=\"w-32 h-32 rounded-full mb-4 border-4 border-white shadow-md\">\n                        <h1 class=\"text-3xl font-bold text-gray-900\">${name}</h1>\n                        <p class=\"text-md text-gray-500\">${role}</p>\n                    </div>\n\n                    <ul class=\"mt-8 space-y-4\">\n                        <li class=\"flex items-center gap-3\">\n                            <i data-lucide=\"linkedin\" class=\"w-5 h-5 text-indigo-500\"></i>\n                            <a href=\"https://${linkedin}\" class=\"text-gray-600 hover:text-indigo-600 hover:underline\">${linkedin}</a>\n                        </li>\n                        <li class=\"flex items-center gap-3\">\n                            <i data-lucide=\"mail\" class=\"w-5 h-5 text-indigo-500\"></i>\n                            <a href=\"mailto:${email}\" class=\"text-gray-600 hover:text-indigo-600 hover:underline\">${email}</a>\n                        </li>\n                        <li class=\"flex items-center gap-3\">\n                            <i data-lucide=\"github\" class=\"w-5 h-5 text-indigo-500\"></i>\n                            <a href=\"https://${github}\" class=\"text-gray-600 hover:text-indigo-600 hover:underline\">${github}</a>\n                        </li>\n                    </ul>\n                </div>\n            </aside>\n\n            <!-- === PROJECT DETAILS === -->\n            <main class=\"w-full lg:w-2/3\">\n                <div class=\"bg-white rounded-xl shadow-lg p-6 md:p-8\">\n                    <section>\n                        <h2 class=\"text-3xl font-bold text-indigo-600 mb-4\">Project Idea: \"${projectTitle}\"</h2>\n                        <p class=\"text-lg text-gray-700 leading-relaxed\">${problem}</p>\n                    </section>\n\n                    <section class=\"mt-8\">\n                        <h3 class=\"text-2xl font-semibold text-gray-900 mb-4\">Tech Stack</h3>\n                        <div class=\"flex flex-wrap gap-3\">\n                            ${techStack.map(t => `\n                              <span class=\"bg-indigo-100 text-indigo-800 text-sm font-medium px-4 py-1.5 rounded-full\">${t}</span>\n                            `).join(\"\")}\n                        </div>\n                    </section>\n\n                    <section class=\"mt-8\">\n                        <h3 class=\"text-2xl font-semibold text-gray-900 mb-4\">Short Guide to Get Started</h3>\n                        <ol class=\"list-decimal list-inside space-y-3 text-gray-700 leading-relaxed\">\n                            ${shortGuide.map(step => `<li>${step}</li>`).join(\"\")}\n                        </ol>\n                    </section>\n                </div>\n            </main>\n        </div>\n    </div>\n\n    <footer class=\"w-full mt-12 mb-8\">\n        <div class=\"container mx-auto max-w-6xl px-8\">\n            <div class=\"flex justify-center gap-6 mb-4\">\n                <a href=\"https://www.linkedin.com/posts/gdgsistec_gdgoncampus-teamgdg-professionalexcellence-activity-7387031483007447040-Zl3W\" class=\"text-gray-500 hover:text-blue-600\" title=\"GDG SISTEC LinkedIn\">\n                    <i data-lucide=\"linkedin\" class=\"w-6 h-6\"></i>\n                </a>\n                <a href=\"https://www.instagram.com/gdg_sistec?igsh=MXdiN20zaW54c3Q3dA==\" class=\"text-gray-500 hover:text-pink-600\" title=\"GDG SISTEC Instagram\">\n                    <i data-lucide=\"instagram\" class=\"w-6 h-6\"></i>\n                </a>\n                <a href=\"https://x.com/gdsc_sistec\" class=\"text-gray-500 hover:text-sky-500\" title=\"GDG SISTEC Twitter\">\n                    <i data-lucide=\"twitter\" class=\"w-6 h-6\"></i>\n                </a>\n            </div>\n\n            <div class=\"border-t border-gray-300 pt-6 text-center\">\n                <p class=\"text-sm text-gray-500 mb-2\">\n                    Created with ‚ù§Ô∏è by Tilak Gupta\n                </p>\n                <a href=\"https://linktr.ee/tilakgupta2005\" target=\"_blank\" class=\"text-sm font-medium text-indigo-600 hover:underline flex items-center justify-center gap-2\">\n                    <i data-lucide=\"link\" class=\"w-4 h-4\"></i>\n                    Connect with the developer\n                </a>\n            </div>\n        </div>\n    </footer>\n\n    <script>lucide.createIcons();</script>\n</body>\n</html>\n`;\n\n// --- Return as downloadable file in n8n ---\nreturn [\n  {\n    json: {\n      email,\n      name,\n      fileName: email.split(\"@\")[0] + \".html\", // unique filename\n      html: htmlContent\n    }\n  }\n];\n"
